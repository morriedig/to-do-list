<ul>
  <% @lists.each do |list| %>
    <li>
      <%= list.title %>
      <%= link_to 'Show', list_path(list) %>
      <%= link_to 'Edit', edit_list_path(list) %>
      <%= link_to 'Destroy', list_path(list), method: :delete, data:{ confirm: "Are you sure?"} %>
    </li>
  <% end %>
</ul>

<%= link_to 'New List', new_list_path %>